{
  "runnableVersion": "01.00.00",
  "name": "runnableHttp",
  "initialDelay": 0,
  "runnables": [
    {
      "timeout": 30000,
      "name": "HttpTitle",
      "subscriptions": [
        {
          "type": "http-server",
          "name": "HttpSubscriptionTitle",
          "endpoint": "/enqueuer",
          "port": 23075,
          "method": "POST",
          "response": {
            "status": 200
          },
          "onMessageReceived": "test['works'] = JSON.parse(message).enqueuer === 'virgs';",
          "timeout": 10000
        }
      ],
      "startEvent": {
        "publisher": {
          "type": "http-client",
          "name": "HttpPublisherClientTitle",
          "url": "http://localhost:23075/enqueuer",
          "method": "POST",
          "payload": {
            "enqueuer": "virgs"
          },
          "headers": {
            "content-type": "application/json"
          }
        }
      }
    }
  ]
}