{
  "$id": "https://github.com/lopidio/enqueuer/runnable",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "title": "The requisition name",
      "default": "",
      "examples": [
        "anyNameOrTitle"
      ]
    },
    "initialDelay": {
      "type": "integer",
      "title": "Initial delay",
      "examples": [
        100
      ]
    },
    "runnableVersion": {
      "type": "string",
      "title": "The runnable version",
      "default": "",
      "examples": [
        "01.00.00"
      ]
    },
    "runnables": {
      "type": "array",
      "minItems": 1,
      "additionalProperties": false,
      "items": {
        "oneOf": [
          {"$ref": "https://github.com/lopidio/enqueuer/runnable"},
          {"$ref": "https://github.com/lopidio/enqueuer/requisition"}
        ]
      }
    },
    "additionalProperties": false
  },
  "required": [
    "runnableVersion",
    "name",
    "runnables"
  ],
  "additionalProperties": false
}